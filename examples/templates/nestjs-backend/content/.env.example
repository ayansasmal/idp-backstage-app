# Application Configuration
PORT=${{ values.port }}
NODE_ENV=development
API_PREFIX=${{ values.apiPrefix }}

{% if values.enableDatabase %}# Database Configuration
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=your_password_here
DATABASE_NAME=${{ values.name | replace('-', '_') }}_db

{% endif %}{% if values.enableAuth %}# AWS Cognito Configuration
AWS_REGION=us-east-1
AWS_COGNITO_USER_POOL_ID=your_user_pool_id_here
AWS_COGNITO_CLIENT_ID=your_client_id_here
JWT_SECRET=your_super_secret_jwt_key_here

{% endif %}# Swagger Documentation
SWAGGER_TITLE=${{ values.name }} API
SWAGGER_DESCRIPTION=${{ values.description }}
SWAGGER_VERSION=1.0.0

# CORS Configuration (comma-separated origins)
CORS_ORIGIN=http://localhost:3000,http://localhost:3001
